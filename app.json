{
  "expo": {
    "sdkVersion": "52.0.0",
    "name": "au-festio-mobile",
    "slug": "au-festio-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/au_festio_logo.jpg",
    "scheme": "au-festio-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/au_festio_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/au_festio_logo.jpg",
      "color": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactnativejs.au-festio-mobile",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access to provide AR navigation.",
        "NSLocationWhenInUseUsageDescription": "This app requires your location to provide navigation assistance.",
        "NSUSERTRACKINGUSAGEDESCRIPTION": "This app uses your data to provide a better user experience.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reactnativejs.au.festio.mobile",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification_icon.png",
          "color": "#ffffff",
          // "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c14ed63-a307-475b-959f-7079cc3cbcde"
      }
    }
  }
}
